<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.IDSearchController">
   <children>
      <Label text="ID">
         <VBox.margin>
            <Insets left="20.0" top="20.0" />
         </VBox.margin></Label>
      <TextField fx:id="ID">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin></TextField>
      <Button fx:id="search" mnemonicParsing="false" onAction="#employeeSearch" text="Pesquisar">
         <VBox.margin>
            <Insets left="20.0" top="20.0" />
         </VBox.margin>
      </Button>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
         <children>
            <TableView fx:id="employeeTable" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
